# APPLICATION CONFIGURATION
NODE_ENV=development
SERVER_PORT=3003
SERVER_HOST=your_server_host_here

# NETWORK CONFIGURATION
# Set this to your actual server IP for network access
# You can use: static IP, hostname, or auto-detect
SERVER_IP=your_server_ip_here
FORCE_HTTPS=false

# SESSION SECURITY  
SESSION_SECRET=your_session_secret_here
# REDIS CONFIGURATION
REDIS_HOST=your_redis_host_here
REDIS_PORT=your_redis_port_here
REDIS_DB=0
# REDIS_PASSWORD=your_redis_password_here

# ADMIN CREDENTIALS
ADMIN_USERNAME=your_admin_username_here
ADMIN_PASSWORD=your_admin_password_here

# GENIEACS CONFIGURATION
GENIEACS_URL=your_genieacs_url_here
GENIEACS_USERNAME=your_genieacs_username_here
GENIEACS_PASSWORD=your_genieacs_password_here

# MIKROTIK CONFIGURATION
MIKROTIK_HOST=your_mikrotik_host_here
MIKROTIK_PORT=your_mikrotik_port_here
MIKROTIK_USER=your_mikrotik_username_here
MIKROTIK_PASSWORD=your_mikrotik_password_here

# RADIUS CONFIGURATION (OPTIONAL)
# RADIUS_HOST=your_radius_host_here
# RADIUS_USER=your_radius_username_here
# RADIUS_PASSWORD=your_radius_password_here
# RADIUS_DATABASE=your_radius_database_here

# WHATSAPP CONFIGURATION
WHATSAPP_DEBUG_LOGS=false
WHATSAPP_SHOW_QR=true
WHATSAPP_ADMINS=your_admin_number_here
TECHNICIAN_NUMBERS=your_technician_number_here
WHATSAPP_KEEP_ALIVE=true
WHATSAPP_RESTART_ON_ERROR=true

# APPLICATION FEATURES
# CUSTOMER_PORTAL_OTP=(true/false) enable/disable OTP login for customer portal
OTP_LENGTH=your_otp_length_here
OTP_EXPIRY_MINUTES=your_otp_expiry_minutes_here
PPPOE_MONITOR_ENABLE=true
RX_POWER_NOTIFICATION_ENABLE=true
RX_POWER_WARNING=you_rx_power_warning_here
RX_POWER_CRITICAL=your_rx_power_critical_here
RX_POWER_WARNING_INTERVAL=your_rx_power_warning_interval_here
RXPOWER_RECAP_ENABLE=your_rxpower_recap_enable_here
RXPOWER_RECAP_INTERVAL=21600000
OFFLINE_NOTIFICATION_ENABLE=true
OFFLINE_NOTIFICATION_INTERVAL=43200000

# PPPOE NOTIFICATIONS
PPPOE_NOTIFICATIONS_ENABLED=true
PPPOE_LOGIN_NOTIFICATIONS=true
PPPOE_LOGOUT_NOTIFICATIONS=true
PPPOE_INCLUDE_OFFLINE_LIST=true
PPPOE_MAX_OFFLINE_LIST_COUNT=20

# TROUBLE REPORT
TROUBLE_REPORT_ENABLED=true
TROUBLE_REPORT_CATEGORIES=Internet Lambat, Tidak Bisa Browsing, WiFi Tidak Muncul, Koneksi Putus-Putus, Lainnya
TROUBLE_REPORT_AUTO_TICKET=true

# COMPANY INFORMATION
COMPANY_HEADER=Your Company Name
FOOTER_INFO=Your Company Footer Info
POW_INFO=Powered by Your Company
DEV_INFO=Note: For more information or any issues on this page, please contact the developer | Dev Contact : +62 812 3456 7890
MAIN_INTERFACE=ETHER 1
USER_AUTH_MODE=you_user_auth_mode_here
